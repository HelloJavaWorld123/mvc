<mapper namespace="com.jzy.api.dao.app.AppInfoMapper">


    <!--    获取商品富文本-->
    <select id="" resultType="com.jzy.api.model.app.AppPage">
        SELECT
            ai_id as aiId,
            content
        FROM
            app_page
        WHERE
            ai_id =#{aiId}
        </select>


</mapper>